<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="coffeebike.salesforce.manager" class="CoffeeBike\SalesforceRESTBundle\Services\SalesforceManager">
            <argument key="$username" tag="coffeebike_salesforce.username"/>
            <argument key="$password" tag="coffeebike_salesforce.password" />
            <argument key="$token" tag="coffeebike_salesforce.token" />
            <argument key="$client_secret" tag="coffeebike_salesforce.client_secret" />
            <argument key="$client_id" tag="coffeebike_salesforce.client_id" />
        </service>
    </services>
</container>